{
    "name": "Barchart Options",
    "description": "Opens a barchart options screener for a given ticker.",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "barchart_options.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Barchart Options"
    },
    "permissions": [
        "clipboardRead",
        "debugger",
        "background"
    ],
    "manifest_version": 2
}