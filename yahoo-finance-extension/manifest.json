{
    "name": "Yahoo Finance Handler",
    "description": "Opens a yahoo finance page with a given ticker.",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "yahoo.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Yahoo Finance Handler"
    },
    "permissions": [
        "clipboardRead",
        "debugger",
        "background"
    ],
    "manifest_version": 2
}